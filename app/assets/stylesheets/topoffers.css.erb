/*LandingPage*/

.lpBody {
    background: #ccc;
    font-family: helvetica, arial, san-serif;
    color: #666;
}

#lpWrapper {
    width: 900px;
    margin: 0 auto;
    background-color: #fff;
    padding: 0px 20px;
}

.lpHeader { 	
	font-size: 1.5em;
	padding: 0px;
	margin: 0px;
}

.topCardBanner {
	margin-top: 0px;
	text-align: center;
	font-size: 1.2em;
	color: #fff;
	padding: 5px 0px 0px;
	border-radius: 3px;
  background-image: url(<%= asset_path "lp/topbannner.png" %>);
  background-repeat: no-repeat;
  height: 40px;
  text-shadow: 0 -1px #0b215d;
	}
	
.topCardBox {
	height: 200px;
    background-image: url(<%= asset_path "lp/topbox.png" %>);
    background-repeat: no-repeat;
}

.lpBottomSection {
	margin-top: 15px;	
}

.compareCards {
	width: 60%;
	float: left;
	height: 360px;
}

.compareTopCardBanner {
	text-align: center;
	font-size: 1em;
	color: #fff;
	border-radius: 3px;
	padding: 5px 0px 0px;
	height: 37px;
	background-image: url(<%= asset_path "lp/comparebannner.png" %>);
	background-repeat: no-repeat;
	text-shadow: 0 -1px #0b215d;
}

.lpCardDetails {
	width: 39%;
	float: right;
	height: 360px;
}

.lpCardDetailsRow {
	border-bottom: 1px solid #DDD;
	padding: 5px 10px;
	font-size: 0.75em;
}

.lpCardDetailsRowLast {
	padding: 5px 10px;
	font-size: 0.75em;
}

.lpCardDetails h3 {
	border: 0px solid #DDD;
	font-size: 1em;
	font-weight: bold;
}

.cardBestForBanner {
	text-align: center;
	font-size: 1em;
	color: #fff;
	border-radius: 3px;
	padding: 5px 0px 0px;
	height: 37px;
	background-image: url(<%= asset_path "lp/detailsbannner.png" %>);
	background-repeat: no-repeat;
	text-shadow: 0 -1px #5e5f61;
}

.lpCardDetailsRow ul, .lpCardDetailsRow ul li {
    margin: 5px 0px 5px 15px;
}
	
.topCardBoxLeft {
	width: 60%;
	padding: 15px 20px;
	float: left;
}

.topCardBoxLeft h1 {
	font-size: 1.45em;
	color: #333;
	letter-spacing: -1px;
	margin-bottom: 0px;
	font-weight: bold;
}

.topCardBoxLeft h2 {
	font-size: .9em;
	color: #0e8b4d;
	margin-bottom: 5px;
	font-weight: bold;
}

.topCardBoxLeft ul, .topCardBoxLeft ul li {
	margin: 5px 0px 10px 10px;
	font-size: 0.95em;
}

.topCardBoxLeft ul li {
	list-style-type: none;
	background: url(<%= asset_path "lp/check-mark.png", :size=> "15x12" %>) no-repeat left;
	padding-left: 15px;
}

.topCardBoxRight {
	width: 30%;
	float: right;
	padding: 20px;
}

.compareCardsTable {
	border: 1px solid #DDD;
	border-radius: 3px;
	margin-top: 3px;
	margin-bottom: 20px;
	height: 325px;
}

.cardDetailsTable {
	border: 1px solid #DDD;
	border-radius: 3px;
	margin-top: 3px;
	background-color: #EEE;
	height: 325px;
}

.cardDetailsTable p {
	margin-top: 7px;
}

.compareTopCardRow {
	border-bottom: 1px solid #DDD;
	height: 83px;
	padding: 10px 0px;
}

.compareTopCardRow h3, .compareTopCardRowLast h3 {
	font-weight: bold;
	color: #203d8d;
	font-size: 0.8em;
}

.compareTopCardRowLast {
	padding: 10px 0px;
}

.compareTopCardRowC2 ul, .compareTopCardRowC2 ul li {
	margin: 5px 0px 0px 10px;
	font-size: 0.85em;
}

.compareTopCardRowC1 {
	width: 20%;
	float: left;
}

.compareTopCardRowC2 {
	width: 60%;
	float: left;
}

.compareTopCardRowC3 {
	width: 20%;
	float: right;
	height: 82px;
	line-height: 77px;
}

.compareTopCardApply {
 	  margin: 30px 0px;
	  padding: 0px;
 	  font-weight: bold;
    font-size: 0.75em;
    width: 95px;
    text-align: center;
   	background-color: #E58D1F;
  	color: #fff;
	  display: block;
	  height: 25px;
	  line-height: 25px;
	  border: 1px solid #E58D1F;
	  border-radius: 3px;
	  webkit-border-radius: 3px;
	  moz-border-radius: 3px;

}

.compareTopCardApply:link, .compareTopCardApply:visited {
    color: #FFF;
}

.compareTopCardApply:hover {
	box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	-webkit-box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	-moz-box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	color: #fff;
	font-weight: normal;
}

.compareTopCardApplyTop {
 	  margin: 10px 0px;
	  padding: 0px;
 	  font-weight: bold;
    font-size: 0.75em;
    width: 95px;
    text-align: center;
   	background-color: #E58D1F;
  	color: #fff;
	  display: block;
	  height: 25px;
	  line-height: 25px;
	  border: 1px solid #E58D1F;
	  border-radius: 3px;
	  webkit-border-radius: 3px;
	  moz-border-radius: 3px;

}

.compareTopCardApplyTop:link, .compareTopCardApplyTop:visited {
    color: #FFF;
}

.compareTopCardApplyTop:hover {
	box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	-webkit-box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	-moz-box-shadow: 0 0 5px rgba(102, 102, 102, 0.6);
	color: #fff;
	font-weight: normal;
}
/*footer*/
.lpFooter {
	clear: both;
	text-align: center;
	font-size: 0.7em;
	padding: 20px 0px 10px 0px;
	height: 100px;
	margin: 0 auto;
	width: 900px;
	}

#privacyFooter {
	width: 850px;
	margin: 0px auto;
	padding: 15px;
	font-size: 0.7em;
	line-height: 1.4em;
	background-color: #eee;
	text-align: left;
}

#privacyFooter h1 {
	font-size: 1.2em;
	font-weight: bold;
}

#privacyFooter h3 {
	font-size: .9em;
	color: #444;
	margin-top: 15px;
	font-weight: bold;
}

#privacyFooter h2 {
	font-size: 1em;
	color: #E58D1F;
	margin-top: 15px;
	font-weight: bold;
}
	
#privacyFooter ul li, #privacyFooter ol li {
	margin-left: 40px;
	background: none;	
}

#privacyFooter ul, #privacyFooter ol {
	background: none;	
}

/*  NFL Landing Page */

/* globalheader
------------------------*/

.nflBody h1 h2 h3 h4 h5 h6 pre code address caption cite code em strong th figcaption { font-size: 1em; font-weight: normal; font-style: normal; }
.nflBody fieldset iframe { border: none; }
.nflBody caption th { text-align: left; }
.nflBody table { border-collapse: collapse; border-spacing: 0; }
.nflBody article aside footer header hgroup nav section figure figcaption { display: block; }


/* globalheader
------------------------*/
.nflBody {
	font: 0.75em/1.5 Helvetica, Arial, 'Liberation Sans', FreeSans, sans-serif;
	color: #000;
	height:1200px;
	background: url(<%= asset_path "lp/nfl/nfl-bg.jpg" %>) 0 0 no-repeat fixed; 
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
}
.nflBody a {
	color: #000;
	text-decoration: none;
	display: block;
}
.nflBody a:hover {
	text-decoration: underline;
}
.nflBody a:focus {
	outline: none;
}
.nflBody p {
	font-size: 1em;
}
.nflBody hr {
	border: 0 #ccc solid; border-top-width: 1px; clear:both; height: 0;
}
.nflBody h1  h2  h3  h4  h5  h6  ol  dl  pre  table  address  fieldset  p  hr {
	margin-bottom: 0px;
}

.nflBody strong {
	font-weight: bold;
}


/* content
------------------------*/

#nflFloater { float:left; height:50%; margin-bottom:-375px;
    width:1px; /* only for IE7 */
}
.nflMain {
    clear: both; 
    height: 695px; 
    position: relative;
}
.nflContainer {
}
.nflPick {
    top: 80px;
    width: 940px;
    margin: 0 auto;
}
.nflPick .nflMiddle {
    background: url(<%= asset_path "lp/nfl/pick-bg.png" %>) repeat-y scroll left top transparent;
    height: auto;
    overflow: hidden;
}
.nflPick .nflTop {
    background: url(<%= asset_path "lp/nfl/pick-top.png" %>) no-repeat scroll left top transparent;
    display: block;
    height: 72px;
    margin-bottom: -20px;
}
.nflPick .nflBottom {
	clear: both;
    background: url(<%= asset_path "lp/nfl/pick-bottom.png" %>) no-repeat scroll left top transparent;
    display: block;
    height: 72px;
    margin-top: -20px;
}
.nflPick .nflMiddle .nflCards {
    background: url(<%= asset_path "lp/nfl/pick-cards.png" %>) no-repeat scroll left top transparent;
    display: block;
    height: 383px;
    margin-left: 60px;
    overflow: auto;
    text-indent: -1000px;
    width: 817px;
}
.nflPick .nflMiddle .nflCards .nflFirst.row .nflCard {
    height: 85px;
}
.nflPick .nflMiddle .nflCards .nflRow .nflCard {
    height: 81px;
}
.nflPick .nflMiddle .nflCards .nflFourth.nflRow .nflCard {
    height: 135px;
}
.nflPick .nflMiddle .nflCards .nflCard {
    float: left;
    width: 102px;
}
.nflPick .nflMiddle .nflCards .nflCard:hover {
    background: none repeat scroll 0 0 #FFFFFF;
    opacity: 0.5;
    z-index: 1;
}
.nflPick .nflHeading {
    font-size: 42px;
    text-align: center;
    text-transform: uppercase;
}
.nflPick .nflHeading .nflFirst {
    color: #F3791F;
    position: relative;
}
.nflPick .nflHeading .nflSecond {
    color: #000;
    position: relative;
}
.nflPick .nflSubhead h2 {
    color: #707070;
    font-size: 16px;
    text-align: center;
    text-transform: uppercase;
    margin-bottom: 10px;
}

.nflSubheadFirst {
    color: #203D8D;
    font-weight: bold;
    text-decoration: underline;
}


/* Card Features and Rewards Section */
.nflDescription {
	margin-left: 110px;
	float:left;
}

.nflFeatures {
	margin: 0px 40px;
	font-size: 1.167em;
	width: 275px;
	float: left;
	border: 1px solid #06095c;
  border-radius: 5px;
  webkit-border-radius: 5px;
  moz-border-radius: 5px;
}

.nflFeaturesDescription {
    border-radius: 0px 0px 4px 4px;
    webkit-border-radius: 0px 0px 4px 4px;
    moz-border-radius: 0px 0px 4px 4px;
}
.nflFeaturesDescription ul {
	height: 75px;
	font-size: 1.1em;
	font-weight: bold;
	color: #444;
	padding-top: 10px;
	padding-left: 40px;
	list-style-image: url(<%= asset_path "lp/check-mark.png" %>);
}

.nflFeaturesTitle{
	font-weight: bold;
	font-size: 1.3em;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background-color: #06095c;
  border-radius: 4px 4px 0px 0px;
  webkit-border-radius: 4px 4px 0px 0px;
  moz-border-radius: 4px 4px 0px 0px;	
  color: #eee;
}

.nflBody .lpFooter {
	font-size: 1em;
	padding: 20px 0px 30px 0px;
	height: 100px;
	width: 900px;
	}

.nflBody .lpFooter a {
	color:#fff;
}


/* END Card Features and Rewards Section */